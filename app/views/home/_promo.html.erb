<div id="template-nt-promo" data-section="promo">
  <div class="container">
    <div class="row text-center">
      <div class="col-md-8 col-md-offset-2 template-nt-section-heading text-center">
        <h2 class="template-nt-lead to-animate-off"> <%= t('video_headline')%> </h2>
        <p class="template-nt-sub to-animate fadeInUp animated"><%= t('video_text')%></p>
      </div>

      <%= video_tag(["FS4Y_#{I18n.locale}.mp4"], style: 'width: 80%', controls: true, preload: "metadata") %>

      <div class="template-nt-spacer template-nt-spacer-sm"></div>

      <div class="col-md-4 col-md-offset-4 text-center to-animate-off">
        <%= link_to t('home_index'), '', class: 'btn btn-success', 'data-nav-section': 'home'%>
      </div>
    </div>
  </div>
</div>