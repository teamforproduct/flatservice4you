<!--  Blog modal  -->
<div class="carousel slide sz_easeOutInCubic fade modal sz_bd_black sz_post_sldr_control sz_post_slider_mod_sld" role="dialog" data-duration="2000" data-interval="false" data-ride="carousel" id="sz_post_slider_6_col">
  <div class="modal-dialog">
    <div class="modal-content">
      <a href="#" class="sz_post_slider_close" data-dismiss="modal">
        <span class="fa fa-times"></span>
      </a>
      <div class="carousel-inner" role="listbox">

        <div class="item active" style="transition-duration: 2000ms;">
          <div class="sz_sldr_post_slider_image">
            <%= image_tag('sliders/Oil_service.jpg') %>
          </div>
          <div class="sz_post_slider_slide_txt">
            <h1><%= t('posts.post_1_title') %></h1>
            <div class="sz_post_slider_slide_author">
              <a href="#">
                <%= image_tag('team_2.jpg') %>
                <span>Tufan</span>
              </a>
              <span>2 months ago</span>
              <%= t('posts.post_1_content').html_safe %>
            </div>
          </div>
        </div>

        <div class="item" style="transition-duration: 2000ms;">
          <div class="sz_sldr_post_slider_image">
            <%= image_tag('sliders/Tire_service.jpg') %>
          </div>
          <div class="sz_post_slider_slide_txt">
            <h1><%= t('posts.post_2_title') %></h1>
            <div class="sz_post_slider_slide_author">
              <a href="#">
                <%= image_tag('team_2.jpg') %>
                <span>Tufan</span>
              </a>
              <span>2 months ago</span>
            </div>
            <%= t('posts.post_2_content').html_safe %>
          </div>
        </div>

        <div class="item" style="transition-duration: 2000ms;">
          <div class="sz_sldr_post_slider_image">
            <%= image_tag('sliders/Small_inspection.jpg') %>
          </div>
          <div class="sz_post_slider_slide_txt">
            <h1><%= t('posts.post_3_title') %></h1>
            <div class="sz_post_slider_slide_author">
              <a href="#">
                <%= image_tag('team_2.jpg') %>
                <span>Tufan</span>
              </a>
              <span>2 months ago</span>
            </div>
            <%= t('posts.post_3_content').html_safe %>
          </div>
        </div>
      </div>
      <a class="carousel-control left" href="#sz_post_slider_6_col" data-slide="prev" role="button">
        <span class="fa fa-angle-left"></span>
      </a>

      <a class="carousel-control right" href="#sz_post_slider_6_col" data-slide="next" role="button">
        <span class="fa fa-angle-right"></span>
      </a>
    </div>
  </div>
</div>

