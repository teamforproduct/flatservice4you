<div id="template-nt-main">
  <div id="template-nt-clients">
    <div class="container">
      <div class="row text-center">
        <div class="col-xs-12 visible-xs-block">
          <figure class="template-nt-client">
            <%= image_tag('logos/uber.png', style: 'margin: 15px')%>
          </figure>
        </div>
        <div class="col-sm-4 col-xs-6">
          <figure class="template-nt-client">
            <%= image_tag('logos/ab.png')%>
          </figure>
        </div>
        <div class="col-sm-4 hidden-xs">
          <figure class="template-nt-client">
            <%= image_tag('logos/uber.png', style: 'margin: 15px')%>
          </figure>
        </div>
        <div class="col-sm-4 col-xs-6">
          <figure class="template-nt-client">
            <%= image_tag('logos/dgw.png')%>
          </figure>
        </div>
      </div>
    </div>
  </div>

  <%= render 'promo'%>
  <%= render 'problem'%>
  <%= render 'solution'%>
  <%= render 'video'%>
  <%= render 'product'%>
  <%= render 'price'%>
  <%= render 'client'%>

  <div id="template-nt-counter" class="template-nt-bg-section" style="background-image: url(<%= image_path('counter.jpg') %>); background-attachment: fixed;">
    <div class="template-nt-overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="template-nt-hero-wrap">
            <div class="template-nt-hero-intro text-center to-animate-off counter-animate">
              <div class="col-md-4 text-center">
                <span class="template-nt-counter js-counter" data-type="weeks"></span>
                <span class="template-nt-counter-label"><%= t('weeks') %></span>
              </div>
              <div class="col-md-4 text-center">
                <span class="template-nt-counter js-counter" data-type="days"></span>
                <span class="template-nt-counter-label"><%= t('days') %></span>
              </div>
              <div class="col-md-4 text-center">
                <span class="template-nt-counter js-counter" data-type="minutes"></span>
                <span class="template-nt-counter-label"><%= t('minutes') %></span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <%= render 'team'%>
  <%= render 'faq'%>
  <%= render 'blog'%>

  <%= render 'users/form' %>
</div>
